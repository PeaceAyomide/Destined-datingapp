{
  "expo": {
    "name": "destined-datingapp",
    "slug": "destined-datingapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/getstartedlogo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/getstartedlogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.peacexv.destineddatingapp",
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/getstartedlogo.png",
        "backgroundColor": "#000000"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "package": "com.peacexv.destineddatingapp",
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "favicon": "./assets/getstartedlogo.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location to find places near you.",
          "locationWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location to find places near you."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/getstartedlogo.png",
          "color": "#ffffff",
          "mode": "production"
        }
      ]
    ],
    "notification": {
      "icon": "./assets/getstartedlogo.png",
      "color": "#ffffff",
      "androidMode": "default",
      "androidCollapsedTitle": "New Message",
      "iosDisplayInForeground": true
    },
    "extra": {
      "eas": {
        "projectId": "74a620dd-acbd-42a1-984d-515bc06af4bc"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/74a620dd-acbd-42a1-984d-515bc06af4bc"
    }
  }
}
